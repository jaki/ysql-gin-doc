\usepackage[margin=1in]{geometry}
\usepackage{booktabs}
\usepackage{enumerate}
\usepackage{hyperref}
\usepackage{mathtools}
\usepackage{minted}
\usepackage{parskip}

% Package: minted.
\setminted{%
    breakanywhere=true,
    breaklines=true,
    fontsize=\small,
}
\newcommand{\setnewminted}[1]{%
    \newminted{#1}{%
        frame=leftline,
    }
}
\newcommand{\setnewmintedfile}[1]{%
    \newmintedfile{#1}{%
        autogobble,
        frame=single,
        label=##2,
    }
}
\newcommand{\setnewmintinline}[1]{%
    \newmintinline{#1}{%
    }
}
\newcommand{\setnewmintall}[1]{%
    \setnewminted{#1}
    \setnewmintedfile{#1}
    \setnewmintinline{#1}
}
\setnewmintall{bash}
\setnewmintall{c}
\setnewmintall{sql}
\setnewmintall{text}

\DeclarePairedDelimiter{\braces}{\lbrace}{\rbrace}
\DeclarePairedDelimiter{\parens}{\lparen}{\rparen}

\newenvironment{iparts}
    {\begin{enumerate}[{(}i{)}]\tightlist}
    {\end{enumerate}}
\newenvironment{nparts}
    {\begin{enumerate}[1.]\tightlist}
    {\end{enumerate}}
\newenvironment{oparts}
    {\begin{itemize}\tightlist}
    {\end{itemize}}
\newenvironment{qparts}
    {\begin{enumerate}[{(}a{)}]\tightlist}
    {\end{enumerate}}

\providecommand{\tightlist}{%
    \setlength{\itemsep}{0pt}\setlength{\parskip}{0pt}}
